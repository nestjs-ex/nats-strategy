{
  "name": "@nestjs-ex/nats-strategy",
  "version": "10.0.2",
  "description": "NATS v2 strategy and client for nestjs microservice",
  "author": "pnthanh",
  "license": "MIT",
  "main": "./index.js",
  "types": "./index.d.ts",
  "scripts": {
    "format": "prettier --write \"**/*.ts\"",
    "lint": "eslint \"lib/**/*.ts\"",
    "lint:fix": "eslint \"lib/**/*.ts\" --fix",
    "test": "jest --config=jest.json",
    "test:watch": "jest --config=jest.json --watch",
    "test:e2e": "jest --config ./e2e/jest-e2e.json",
    "test:coverage": "jest --config=jest.json --coverage --coverageDirectory=coverage",
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.json",
    "precommit": "lint-staged",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prerelease": "npm run build",
    "release": "release-it"
  },
  "peerDependencies": {
    "@nestjs/common": "^7.6.15 || ^8.0.0 || ^9.0.0 || ^10.0.0",
    "@nestjs/microservices": "^7.6.15 || ^8.0.0 || ^9.0.0 || ^10.0.0"
  },
  "dependencies": {
    "nanoid": "3.3.7",
    "nats": "^2.18.0"
  },
  "devDependencies": {
    "@commitlint/cli": "20.4.2",
    "@commitlint/config-angular": "20.4.2",
    "@nestjs/common": "10.4.22",
    "@nestjs/core": "10.4.22",
    "@nestjs/microservices": "^10.2.8",
    "@nestjs/platform-express": "^10.2.8",
    "@nestjs/testing": "10.4.22",
    "@types/jest": "30.0.0",
    "@types/node": "24.10.13",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "chai": "^4.3.10",
    "eslint": "8.57.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "husky": "9.1.7",
    "jest": "30.2.0",
    "lint-staged": "16.2.7",
    "prettier": "3.8.1",
    "reflect-metadata": "0.2.2",
    "release-it": "19.2.4",
    "rimraf": "6.1.3",
    "supertest": "^7.0.0",
    "ts-jest": "29.4.6",
    "typescript": "5.9.3"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -c .commitlintrc.json -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nestjs-ex/nats-strategy"
  },
  "publishConfig": {
    "access": "public"
  }
}
