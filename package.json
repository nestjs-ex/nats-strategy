{
  "name": "@nestjs-ex/nats-strategy",
  "version": "9.0.2",
  "description": "NATS v2 strategy and client for nestjs microservice",
  "author": "pnthanh",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"**/*.ts\"",
    "lint": "eslint \"lib/**/*.ts\" --fix",
    "test": "jest --config=jest.json",
    "test:watch": "jest --config=jest.json --watch",
    "test:e2e": "jest --config ./e2e/jest-e2e.json",
    "test:coverage": "jest --config=jest.json --coverage --coverageDirectory=coverage",
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.json",
    "precommit": "lint-staged",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prerelease": "npm run build",
    "release": "release-it"
  },
  "peerDependencies": {
    "@nestjs/common": "^7.6.15 || ^8.0.0 || ^9.0.0",
    "@nestjs/microservices": "^7.6.15 || ^8.0.0 || ^9.0.0",
    "nats": "^2.1.0"
  },
  "dependencies": {
    "nanoid": "3.3.6",
    "nats": "^2.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.7",
    "@commitlint/config-angular": "17.6.7",
    "@nestjs/common": "9.4.3",
    "@nestjs/core": "9.4.3",
    "@nestjs/microservices": "^9.4.1",
    "@nestjs/platform-express": "^9.4.1",
    "@nestjs/testing": "9.4.3",
    "@types/jest": "29.5.3",
    "@types/node": "20.4.6",
    "@typescript-eslint/eslint-plugin": "6.2.1",
    "@typescript-eslint/parser": "6.2.1",
    "chai": "^4.3.7",
    "eslint": "8.46.0",
    "eslint-config-prettier": "8.9.0",
    "eslint-plugin-import": "2.28.0",
    "husky": "8.0.3",
    "jest": "29.6.2",
    "lint-staged": "13.2.3",
    "prettier": "3.0.0",
    "reflect-metadata": "0.1.13",
    "release-it": "16.1.3",
    "rimraf": "5.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.1",
    "typescript": "5.1.6"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -c .commitlintrc.json -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nestjs-ex/nats-strategy"
  },
  "publishConfig": {
    "access": "public"
  }
}
